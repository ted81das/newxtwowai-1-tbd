                "openTypography": 0,
                "size": {
                    "md": "",
                    "unit": "px"
                }
            },
            "style": [
                {
                    "selector": "{{PLUS_WRAP}} .tpgb-toc .toc-list > li > a"
                }
            ],
            "scopy": true
        },
        "Level1NormalColor": {
            "type": "string",
