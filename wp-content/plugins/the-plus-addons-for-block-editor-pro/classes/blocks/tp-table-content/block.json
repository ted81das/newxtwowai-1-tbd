{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"name": "tpgb/tp-table-content",
	"version": "0.1.0",
	"title": "Table of Contents",
	"category": "tpgb",
	"description": "List of the main headings of your blogs or page for easy navigation and readability.",
