            "type": "string",
            "default": ""
        },
        "typeList": {
            "type": "string",
            "default": "UL"
        },
        "Style": {
            "type": "string",
            "default": "none"
        },
        "selectorH1": {
            "type": "boolean",
            "default": true
        },
        "selectorH2": {
            "type": "boolean",
            "default": true
        },
        "selectorH3": {
            "type": "boolean",
            "default": true
        },
        "selectorH4": {
            "type": "boolean",
            "default": true
        },
        "selectorH5": {
            "type": "boolean",
            "default": true
        },
        "selectorH6": {
            "type": "boolean",
            "default": true
        },
        "contentSelector": {
            "type": "string",
