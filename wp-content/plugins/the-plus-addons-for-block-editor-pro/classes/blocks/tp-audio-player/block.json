            },
            "style": [{
                "selector": "{{PLUS_WRAP}} .tpgb-audioplay-wrap{max-width:{{MaxWidth}};margin:0 auto;}"
            }],
            "scopy": true
        },
        "DefaultVolume": {
            "type": "string",
            "default": ""
