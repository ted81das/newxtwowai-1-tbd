{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"name": "tpgb/tp-audio-player",
	"version": "0.1.0",
	"title": "Audio Player",
	"category": "tpgb",
	"description": "Stream your audio, create playlists directly from your site",
	"example": {
		"attributes": {}
	},
	"supports": {
		"align": ["wide","full"]
