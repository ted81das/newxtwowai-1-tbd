            "type": "boolean",
            "default": false,
            "scopy": true
        },
        "chartHeight": {
            "type": "object",
            "default": {
                "md": "",
                "unit": "px"
            },
            "style": [
                {
                    "condition": [
                        {
                            "key": "mAspctRatio",
                            "relation": "==",
                            "value": true
                        }
                    ],
                    "selector": "{{PLUS_WRAP}} { height: {{chartHeight}}; }"
                }
            ],
            "scopy": true
        },
        "animation": {
            "type": "string",
            "default": "easeOutQuart",
            "scopy": true
        },
        "anDuration": {
            "type": "string",
            "default": "1000",
            "scopy": true
