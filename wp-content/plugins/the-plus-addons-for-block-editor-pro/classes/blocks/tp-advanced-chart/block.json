{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"name": "tpgb/tp-advanced-chart",
	"version": "0.1.0",
	"title": "Advanced Chart",
	"category": "tpgb",
	"description": "Show your complex data with detailed charts and analytics for better understanding",
	"example": {
