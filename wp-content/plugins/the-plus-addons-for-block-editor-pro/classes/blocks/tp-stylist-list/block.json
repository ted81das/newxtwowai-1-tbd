            "default": [
                {
                    "_key": "0",
                    "description": "List item 1",
                    "selectIcon": "fontawesome",
                    "iconFontawesome": "fas fa-check-circle",
                    "tooltipTypo": {
                        "openTypography": 0
                    },
                    "tooltipText": "",
                    "hintText": "",
                    "descurl": {
                        "url": "",
                        "target": "",
