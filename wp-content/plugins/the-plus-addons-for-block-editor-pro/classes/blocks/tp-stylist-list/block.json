                    "selector": "{{PLUS_WRAP}} .tpgb-icon-list-items, {{PLUS_WRAP}} .tpgb-icon-list-items .tpgb-icon-list-item{flex-wrap: wrap;flex-flow: wrap;}  {{PLUS_WRAP}} .tpgb-icon-list-items .tpgb-icon-list-item{ margin : 0px }"
